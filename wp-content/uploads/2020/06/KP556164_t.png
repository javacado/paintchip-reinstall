  <head>
    <meta charset="ISO-8859-1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width">
    <meta http-equiv="cache-control" content="no-cache, no-store, must-revalidate">
    <meta http-equiv="expires" content="0">
    <meta http-equiv="pragma" content="no-cache">
    <title>Home | MacPherson's</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <style>
      body {
        font-family: franklin-gothic-urw, sans-serif;
        text-align: center;
        font-weight: 400;
        color: #444444;
      }
      hr {
        margin: 0;
      }
      .content {
        box-shadow: 1px 1px 5px 3px rgba(232,227,232,0.43);
        max-width: 1200px;
        margin: 0 auto;
        padding-bottom: 15px;
      }

      .retailer-login-button {
        display: inline-block;
        float: right;
        background-color: #3392b0;
        border: 1px solid #7c7c7c;
        min-width: 130px;
        max-height: 42px;
        padding: 10px 12px;
        margin: 30px 20px;
        letter-spacing: -0.5px;
      }
      .retailer-login-button:hover {
        cursor: pointer;
        text-decoration: none;
      }
      .retailer-login-button:visited {
        text-decoration: none;
      }
      .retailer-login-copy {
        color: #FFFFFF;
        font-size: 13px;
        font-weight: 400;
        line-height: 20px;
        text-decoration: none;
        text-shadow: 0 1px 0 rgba(255, 255, 255, .25);     
      }
      .mac-content {
        min-height: 400px;
      }
      .greeting-header {
        background-color: #3392b0;
        padding: 0.4rem;
        text-align: right;
        color: white;
        height: 26px;
      }
      .mac-logo-container {
        max-width: 150px;
        display: inline-block;
        float: left;
      }
      .mac-logo {
        width: 100%
        height: auto;
        padding: 20px 5px;
      }
      .mac-logo:hover {
        cursor: pointer;
      }
      .ooops-header {
        font-size: 42px;
        line-height: 58px;
        margin: 30px 15px 0;
        padding: 0 20px;
      }
      .ooops-body {
        font-size: 20px;
        line-height: 30px;
        max-width: 700px;
        text-align: center;
        margin: 20px auto;
        padding: 0 20px;
      }
      .try-again-button {
        display: inline-block;
        background-color: #3392b0;
        min-width: 115px;
        max-height: 36px;
        padding: 8px;
        margin: 10px 5px;
      }
      .try-again-button:hover {
        background-color: #3392b0;
        text-decoration: none;
        cursor: pointer;
      }
      .try-again-copy {
        color: #FFFFFF;
        font-size: 14px;
        line-height: 20px;
        font-weight: 600;
        text-decoration: none;
      }
      .footer-container * {
        color: #787878;
      }
      .footer-container {
        width: 100%;
        height: 8rem;
        color: #888;
        font-size: 1.3rem;
        border-top: 1px solid #dedede;
        margin-bottom: 40px;
      }
      .important-links-section {
        display: inline-block;
        float: right;
      }
      .important-links-column {
        display: inline-block;
      }
      .important-link {
        display: inline-block;
        color: #787878;
        font-size: 11px;
        line-height: 37px;
        font-weight: 400;
        margin: 0 0 0 6px;
      }
      .social-media-section {
        display: inline-block;
        float: left;
      }
      .social-media-link {
        display: inline-block;
        max-width: 15px;
        max-height: 15px;
        margin: 5px 0;
      }
      .social-media-logo {
        max-width: 15px;
        max-height: 15px;
        vertical-align: baseline;
      }
      .copyright-year-container {
        text-align: right;
        min-width: 100%;
        float: right;
      }
      .copyright-year {
        color: #787878;
        font-size: 11px;
        line-height: 37px;
        font-weight: 400;
      }
      #footer {
          margin-top: 20px;
          padding: 0 65px;
      }
      @media (max-width: 991px) {
        .important-link {
          line-height: 11px;
          text-align: center;
          margin: 0 3px;
        }
        .social-media-section {
          min-width: 100%;
          float: none;
          text-align: center;
        }
        .important-links-column {
          width: 100%;
        }
        .important-links-section {
          float: none;
        }
        .copyright-year-container {
          min-width: 100%;
          float: none;
          text-align: center;
        }
        .copyright-year {
          line-height: 16px;
        }
      }
      @media (max-width: 500px) {
        .retailer-login-button {
          padding: 8px;
          min-width: 115px;
        }
        .retailer-login-button:hover {
          cursor: pointer;
        }
        .retailer-login-copy {
          font-size: 11px;
          line-height: 16px;
        }
        .ooops-header {
          font-size: 38px;
        }
        .ooops-body {
          font-size: 14px;
        }
      }
    </style>
